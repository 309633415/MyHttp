<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
  <package name="relationship" extends="struts-default,json-default" namespace="/relationship" >
    <action name ="relationShipAction" class ="demoinfo.hibernate.relationship.RelationShipAction" method="relationshipDetail">    
        <result name="fail">/common/fail.jsp</result>
         <result name="success">/common/success.jsp</result>   
     </action> 
    <action name ="queryLinkList" class ="demoinfo.hibernate.relationship.RelationShipAction" method="doQueryLinkList">    
         <result name="success">/hibernate/hibernateQueryLink.jsp</result>   
     </action> 
     
     	<action name="commonQuery"
			class="demoinfo.hibernate.relationship.RelationShipAction" method="doCommonQuery">
			<result name="fail"></result>
				<result type="json"><param name="root">result</param>
			</result>
		</action>
		
     	<action name="conditionQuery"
			class="demoinfo.hibernate.relationship.RelationShipAction" method="doConditionQuery">
			<result name="fail"></result>
				<result type="json"><param name="root">result</param>
			</result>
		</action>
		
     	<action name="originalQuery"
			class="demoinfo.hibernate.relationship.RelationShipAction" method="doOriginalQuery">
			<result name="fail"></result>
				<result type="json"><param name="root">result</param>
			</result>
		</action>
		
     	<action name="criteriaQuery"
			class="demoinfo.hibernate.relationship.RelationShipAction" method="doCriteriaQuery">
			<result name="fail"></result>
				<result type="json"><param name="root">result</param>
			</result>
		</action>
  </package>
</struts>