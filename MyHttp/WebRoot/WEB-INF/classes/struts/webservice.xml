<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
  <package name="webservice" extends="struts-default" namespace="/webservice">
      <action name="take" class="xfireAction" method="webService">
            <result>/webservice/webServiceTake.jsp</result>
      </action>
      <action name="httpclient" class="httpclientAction" >
            <result>/webservice/webServiceHttpClient.jsp</result>
      </action>
      <action name="xstream" class="xstreamAction" >
            <result>/webservice/webServiceXstream.jsp</result>
      </action>
      <action name="soap"  class="soapAction" >
            <result>/webservice/webServiceSoap.jsp</result>
      </action>
  </package>
</struts>